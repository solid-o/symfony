<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Solido\QueryLanguage\Form\FieldType" id="Solido\QueryLanguage\Form\FieldType">
            <argument type="service" id="translator" on-invalid="null" />
            <tag name="form.type" />
        </service>

        <service class="Solido\QueryLanguage\Form\PageTokenType" id="Solido\QueryLanguage\Form\PageTokenType">
            <tag name="form.type" />
        </service>

        <service class="Solido\QueryLanguage\Form\QueryType" id="Solido\QueryLanguage\Form\QueryType">
            <tag name="form.type" />
        </service>
    </services>
</container>
